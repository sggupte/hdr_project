function m = isMonotonic(f)
    i1 = 1;
    i2 = 2;
    while(f(i1) == f(i2))
        
    end
        

    incr = true;
    m = 1;
    for i = 1:(length(f)-1)
        if(~incr && (f(i)>=f(i+1)))
            m = 0;
            break;
        end
        if(incr && (f(i)<=f(i+1)))
            m = 0;
            break;
        end
    end

end